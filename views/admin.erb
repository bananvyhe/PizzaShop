<table class="table table-striped"> 
<thead> 
		<tr> 
		 
		<th>Name</th> 
		<th>Phone</th> 
		<th>Address</th> 
		<th>Order</th> 
		</tr> 
		</thead> 
	<tbody> 
		<% @orders.each do |item| %>
			<tr> 
			 
			<td><%= item.name %></td> 
			<td><%= item.phone %></td> 
			<td><%= item.address %></td> 
			<td><%= item.order %></td> 

			</tr> 
			
		<% end %>
	</tbody>
</table>